#    set smtp_pass = '<pass>'
set imap_pass = `security find-generic-password -w -s muttgmail -a andreasmartenhallberg@gmail.com`
set smtp_pass = `security find-generic-password -w -s muttgmail -a andreasmartenhallberg@gmail.com`
set imap_user = 'andreasmartenhallberg@gmail.com'

set from = "andreasmartenhallberg@gmail.com"
set folder = imaps://imap.gmail.com:993
set spoolfile = "+INBOX"
set smtp_url = "smtps://andreasmartenhallberg@smtp.gmail.com:465/"
mailboxes "+[Gmail]/Sent Mail" +INBOX
set record = "+[Gmail]/Sent Mail"
set header_cache = "~/.mutt/gmail/cache/headers"
set message_cachedir = "~/.mutt/gmail/cache/bodies"
set certificate_file = ~/.mutt/certificates
