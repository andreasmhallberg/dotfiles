	Keywords = {Arabic language, {Arabic} language, {EGYPT}, {EGYPT}, {LANGUAGE} \& languages, {LANGUAGE} \& languages, {MIDDLE} East, {MIDDLE} East, Semitic languages, Semitic languages},
