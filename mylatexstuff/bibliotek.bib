	Issn = {0010-9452},
	Journaltitle = {Cortex},
	Keywords = {Arabic, Hemispheric asymmetries, Split fovea, Word recognition},
	Number = {8},
	Pages = {992--997},
	Shortjournal = {Cortex},
	Shorttitle = {Evaluating hemispheric divisions in processing fixated words},
	Title = {Evaluating Hemispheric Divisions in Processing Fixated Words: The Evidence from {Arabic}},
	Urldate = {2015-11-25},
	Volume = {47},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0010945211000372},
