@article{abdulaziz_factors_1986,
	Abstract = {This article discusses the factors involved in the modernization of the {Arabic} language in {Egypt} and the Middle East. Modernization of the Arabic language in {Egypt} got its impetus through the modernization policies of Mohamed Ali, who encouraged young Egyptians to go to Europe to study scientific and technical subjects. When the students returned home, they were in the vanguard of the modernization movement and a Western cultural style of living, which contributed to the trend for the modernization of the Arabic language. In 1812 Arabic was declared the national language of Egypt. On the assumption that major changes in language structure and use reflect closely what goes on elsewhere in the sociocultural, literary, intellectual, educational and psychological life of society, it follows that these various factors impinge upon the modernization process in general and language use and development in particular. In Lebanon, modernizing influences started early in the 19th century because of Arab-Christian ties to European churches and the much greater presence of Europeans. The first Arabic press is said to have been introduced by a Maronite scholar in the 17 century. In the 19th and 20th centuries, Lebanon was in the vanguard of the enlightenment movement, with its modern press producing all kinds of modern Arabic literary and scientific materials and specialized dictionaries.},
	Author = {Abdulaziz, Mohamed H.},
	Date = {1986},
	File = {EBSCO Full Text:/Users/andreashallberg/Library/Application Support/Firefox/Profiles/weghe6py.default/zotero/storage/29UDQ5I3/Abdulaziz - 1986 - Factors in the development of modern {Arabic} usage.pdf:application/pdf;EBSCO Full Text:/Users/andreashallberg/Library/Application Support/Firefox/Profiles/weghe6py.default/zotero/storage/42WJ5VS2/Abdulaziz - 1986 - Factors in the development of modern Arabic usage.pdf:application/pdf},
	Issn = {01652516},
