@article{mughazy_reading_2005,
	Abstract = {This paper investigates the effects of the vowel marking (or the lack thereof) on the reading strategies used by adult educated native speakers, and how these strategies can enhance the reading skills of learners of Arabic as foreign language. Twenty-four college-educated native speakers were engaged in a context reconstruction task to test their ability to disambiguate homographs in three conditions: when the script included accurate vowel diacritics, when the diacritics were misleading, and when they were omitted. The results demonstrate that proficient readers of Arabic employ top-down strategies more frequently and efficiently than bottom-up strategies. In other words, they do not make significant use of vowel marking when made available. Rather, they rely on their knowledge of the content, morpho-phonemic patterns, and intuitions about the statistical probabilities of word order to disambiguate homographs. These patterns suggest that learners of Arabic need context based strategy instruction rather than reliance on text driven bottom-up strategies that can hinder their development as readers.},
	Author = {Mughazy, Mustafa},
	Date = {2005},
	Date-Modified = {2017-03-20 09:48:03 +0000},
	Issn = {0889-8731},
	Journaltitle = {al-ʿArabiyya},
