	langid = {english},
	keywords = {External Source, High Education, Learning Activity, Mental Model, Teaching Practice},
	file = {Springer Full Text PDF:/Users/xhalaa/Zotero/storage/GM2L39IB/Vermunt - 1996 - Metacognitive, cognitive and affective aspects of .pdf:application/pdf}
}

@article{vermunt_analysis_1988,
