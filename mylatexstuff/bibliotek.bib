	journaltitle = {Language in Society},
	langid = {english},
	number = {3},
	pages = {359--385},
	shortjournal = {Lang. Soc.},
	shorttitle = {Standardization or restandardization},
	title = {Standardization or restandardization: The case for ``Standard'' Spoken Tamil},
	urldate = {2019-11-28},
