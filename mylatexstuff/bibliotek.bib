@article{handal_optimal_2005,
	title = {Optimal use of feedback in research supervision with master and doctoral students},
	volume = {25},
	issn = {1891-5949, 1891-5914},
	abstract = {Research supervision involves giving feedback to students, whether they are doctoral or master students. The students produce some kind of text, often a draft, and share it with their supervisor who is expected to assess it. But how is feedback best given? What kind of feedback might be productive for the student and what might not? Since research supervision most often takes place behind closed doors, as private communication between student and supervisor, there has not been much sharing and exchange of methods and experience between and among supervisors. Nowadays, however, supervisors seem keen to learn more about how to deal with this important task. And students, rather than leaving it all to the supervisor's judgement, seem eager to. In this article we offer practical advice for the consideration of supervisors as well as students. It is based partly on research and partly on information shared with us by our co-supervisors.},
	pages = {177--190},
	number = {3},
	journaltitle = {Nordic Studies in Education},
