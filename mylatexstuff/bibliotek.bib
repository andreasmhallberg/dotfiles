@article{belnap_arabic_2003,
	abstract = {During the past two decades a good deal of research has focused on intermediate forms of spoken {Arabic}; little attention, however, has been paid to the mixing found in the everyday prose of modern Arabs. This paper explores aspects of the use of {Arabic} dialectal elements that are regularly used and more formal structures that are regularly avoided in informal personal correspondence. It is based on interviews conducted primarily with Jordanians, Palestinians, Egyptians, and Moroccans, as well as the analysis of samples of written correspondence. This paper examines some of the factors that underlie an apparent trend to use dialectal Arabic in an increasing number of domains formerly considered the domain of "standard" Arabic. Key factors include the spread of basic literacy among the masses as well as contact with Western rhetorical models styles. Particular discussion is devoted to the role of language at attitudes. [{ABSTRACT} {FROM} {AUTHOR}]
