@incollection{janda_inflectional_2010,
	title = {Inflectional Morphology},
	abstract = {In terms of both form and meaning, inflectional morphology occupies an unusual position in language, as it teeters on the margins between lexicon and syntax in apparent defiance of definition. In most languages, inflectional morphology marks relations such as person, number, case, gender, possession, tense, aspect, and mood, serving as an essential grammatical glue holding the relationships in constructions together. Yet in some languages, inflectional morphology is minimal or may not exist at all. From the perspective of cognitive linguistics, inflectional morphology presents a rich array of opportunities to apply and test core concepts, particularly those involving category structure (radial categories, prototypicality, polysemy), the grounding and organization of categories (embodiment, basic-level concepts, “ception,” construal), and the means of extension and elaboration of categories (metaphor, metonymy). This article assumes that there are three kinds of morphemes: lexical, derivational, and inflectional. The behavior of these three types of morphemes can best be understood within the context of constructions. The article focuses on the highly inflected Slavic languages to illustrate the discussions.},
	pages = {632--649},
	booktitle = {The Oxford Handbook of Cognitive Linguistics},
	author = {Janda, Laura},
	editor = {Geeraerts, Dirk and Cuyckens, Hubert},
	urldate = {2019-04-16},
	date = {2010},
	langid = {english},
	file = {Snapshot:/Users/xhalaa/Zotero/storage/TG774QBC/oxfordhb-9780199738632-e-24.html:text/html}
}

@article{boussofaraomar_neither_2006,
	title = {Neither third language nor middle varieties but diglossic switching},
	issn = {0170-026X},
	url = {https://www.jstor.org/stable/43525795},
	abstract = {The goal of this paper is to describe the nature of syntactic constraints on diglossie switching between the two varieties of Arabic in light of Myers-Scotton's Matrix Language Frame Model (1993a, [1997]). Although there has been interest in the nature of intermediate varieties of the Arabic language, i.e. those involving some mix of Classical Arabic ({CA}), Modern Standard Arabic ({MSA}), and national dialects, research on these varieties has generally been limited to describing the linguistic features of a third language or middle varieties. Absent, however, has been any effort to analyze the mix as switching between {CA}/{MSA} and the dialect or to relate issues of variation to theoretical models of switching. In this paper, I argue that what is called third language (or middle varieties) are in fact diglossie switching, and that there is no variety conventionalized as third language. Rather, what is being conventionalized are patterns of switching between two varieties with the dialect as the matrix variety into which constituents (i.e. content morphemes) from {CA}/{MSA} are embedded. The data examined in light of the {MLF} model demonstrate that the mix of the two varieties is governed by structural constraints and hence the switching patterns are systematic and predictable.},
	pages = {55--80},
	number = {45},
	journaltitle = {Zeitschrift für Arabische Linguistik},
	author = {Boussofara-Omar, Naima},
	urldate = {2019-09-26},
	date = {2006}
}


