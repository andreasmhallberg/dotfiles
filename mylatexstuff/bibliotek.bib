@article{mitchell_what_1986,
	Abstract = {The author argues that stylistic and also regional distinctions can and must be included in grammars of standard spoken languages of the educated {Arabic} type. In face of so much regional and stylistic variation, the urge to koineize Arabic speech under the powerful influences, emotive and other, of nationalism, mass education and urbanization is in large measure served by the controlling power of the written language. It is the interplay between written Arabic and vernacular Arabic that creates and maintains Educated Spoken Arabic ({ESA}), both nationally and internationally. The label Modern Standard Arabic ({MSA}) is commonly applied to the written language of contemporary literature, journalism, television and radio news broadcasts, scientific and technological writing, administration and diplomacy. The lexicogrammatical regularities of {ESA}, national or international, are not those of {MSA}. Thus, no gender distinctions are made in {ESA} in the 2nd and 3rd persons plural and a non-{MSA} type of gender distinction is made in the 2nd person singular. The famous High versus Low dichotomy of the classic formulation of so-called diglossia is one such rather coarsely meshed net which provides no adequate basis for a grammatical description of {ESA}.},
	Author = {Mitchell, Terence Frederick},
	Date = {1986},
	Issn = {01652516},
	Journaltitle = {International Journal of the Sociology of Language},
	Pages = {7--32},
	Title = {What is {Educated} {Spoken} {Arabic}?},
	Volume = {61}}

